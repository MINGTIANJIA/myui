/* 水平间距 */
.cate {
  display: flex;
  background-color: #fff;
}
.cate .cate_nav {
  width: 100px;
  line-height: 60px;
  text-align: center;
  background-color: #fff;
}
.cate .cate_nav.active {
  position: relative;
}
.cate .cate_nav.active::before {
  content: '';
  display: block;
  background-color: #c00000;
  width: 3px;
  height: 30px;
  position: absolute;
  top: 50%;
  left: 3px;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}
.cate .leveltwo {
  line-height: 40px;
  text-align: center;
  background-color: #fff;
}
.cate .levelthree {
  display: flex;
  flex-wrap: wrap;
  text-align: center;
}
.cate .levelthree image {
  margin: 5px;
  width: 80px;
  height: 80px;
}

